{
    "name" : "aescarcha/async",
    "description" : "A bundle to handle serialization and deseralization of Entities on Symfony, it's a layer over Symfony's Serializer Component",
    "type" : "symfony-bundle",
    "authors" : [{
        "name" : "Álvaro Escarcha"
    }],
    "keywords" : [
        "symfony, serialization"
    ],
    "license" : [
        "MIT"
    ],
    "require" : {
        "php": ">=5.5.9",
        "symfony/dependency-injection" : "~2.3 || ~3.0",
        "doctrine/orm": "^2.4.8",
        "doctrine/doctrine-bundle": "~1.4",
        "oldsound/rabbitmq-bundle": "1.*"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "~2.7",
        "phpunit/phpunit":              "~4.8|~5.0"
    },
    "autoload" : {
        "psr-0" : {
            "Aescarcha\\SerializerBundle" : ""
        }
    },
    "target-dir" : "Aescarcha/SerializerBundle"
}